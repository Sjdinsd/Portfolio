{
  "name": "Portfolio",
  "tree": {
    "$ignoreUnknownInstances": true,
    "$className": "DataModel",
    "ReplicatedStorage": {
      "Shared": {
        "$ignoreUnknownInstances": true,
        "$path": "src/shared",
        "ModuleLoader": {
          "$className": "ModuleScript",
          "serverSingletonLoader": {
            "$className": "Script",
            "$properties": {
              "RunContext": "Legacy"
            }
          }
        }
      },
      "Packages": {
        "$path": "Packages"
      },
      "Assets": {
        "$className": "Folder",
        "$ignoreUnknownInstances": true
      }
    },
    "ServerScriptService": {
      "$path": "src/server",
      "ServerPackages": {
        "$path": "ServerPackages"
      }
    },
    "StarterPlayer": {
      "StarterPlayerScripts": {
        "$path": "src/client"
      }
    }
  }
}